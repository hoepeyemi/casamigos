{
  "name": "casamigos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "node -e \"const fs = require('fs'); const path = require('path'); const src = path.join(__dirname, '..', 'ignition', 'deployments', 'chain-5003', 'deployed_addresses.json'); const dest = path.join(__dirname, 'src', 'deployed_addresses.json'); if (fs.existsSync(src)) { fs.copyFileSync(src, dest); console.log('✅ Copied deployed_addresses.json'); } else { console.warn('⚠️  deployed_addresses.json not found, skipping copy'); }\"",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "vercel build --prod && vercel deploy --prebuilt --prod",
    "deploy:preview": "vercel build && vercel deploy --prebuilt"
  },
  "dependencies": {
    "bignumber.js": "^9.3.1",
    "ethers": "^6.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.6.0",
    "thirdweb": "^5.73.0",
    "viem": "^2.21.52",
    "vite-plugin-node-polyfills": "^0.24.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/history": "^4.7.11",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
